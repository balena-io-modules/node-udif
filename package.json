{
  "name": "@balena/udif",
  "version": "1.0.0",
  "description": "Apple Universal Disk Image Format (UDIF/DMG)",
  "license": "MIT",
  "author": "Jonas Hermsmeier <jhermsmeier@gmail.com> (https://jhermsmeier.de)",
  "contributors": [
    "Jonas Hermsmeier <jhermsmeier@gmail.com> (https://jhermsmeier.de)",
    "Alexis Svinartchouk <alexis@balena.io>"
  ],
  "keywords": [
    "apple",
    "disk",
    "image",
    "disk-image",
    "dmg",
    "koly",
    "mish",
    "udif",
    "universal"
  ],
  "files": [
    "build",
    "typings"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.ts": [
      "npm run lint-fix"
    ]
  },
  "main": "build/index.js",
  "dependencies": {
    "apple-data-compression": "^0.4.1",
    "apple-plist": "^0.3.0",
    "cyclic-32": "^1.1.0",
    "unbzip2-stream": "^1.4.3"
  },
  "devDependencies": {
    "@balena/lint": "^5.1.0",
    "@types/mocha": "^8.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "homepage": "https://github.com/balena-io-modules/node-udif",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io-modules/node-udif.git"
  },
  "bugs": {
    "url": "https://github.com/balena-io-modules/node-udif/issues"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "mocha -r ts-node/register test/*.spec.ts",
    "lint": "balena-lint --typescript lib test",
    "lint-fix": "balena-lint --fix --typescript lib test"
  }
}
